<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:bulletin="clr-namespace:Prolizy.Viewer.Controls.Bulletin"
             xmlns:viewModels="clr-namespace:Prolizy.Viewer.ViewModels"
             xmlns:avalonia="clr-namespace:LiveChartsCore.SkiaSharpView.Avalonia;assembly=LiveChartsCore.SkiaSharpView.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="viewModels:BulletinPaneViewModel"
             x:Class="Prolizy.Viewer.Controls.Bulletin.GraphDisplay">
    <avalonia:PolarChart
        Series="{Binding Series}"
        AngleAxes="{Binding AngleAxes}"
        LegendPosition="Top"
        LegendTextSize="16"
        RadiusAxes="{Binding RadialAxes}">
    </avalonia:PolarChart>
</UserControl>
